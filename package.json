    "@tkesgar/sharo-next": "^0.3.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "http-errors": "^1.7.3",
    "ky": "^0.15.0",
    "ky-universal": "^0.3.0",
    "lodash": "^4.17.15",
